const
  { test, fetch, read }
    = require ('snuggsi')


test `Payload from test json`

( async assert => {

  let
    payload = await fetch `https://jsonplaceholder.typicode.com/todos/1`
  , json    = await payload.json ``
  , text    = await read `index.go`

  assert (text)
  assert ('title' in json)
})
