{
  "version": 2,
  "name": "studio",
  "alias": ["devpunks"],
  "scope": "devpunks",

  "routes": [
    {
      "src": "/(.*).manifest",
      "headers": {
        "content-type": "application/json"
        },
      "dest": "/$1.manifest"
    }
  ],

    "builds": [
      { "src": "*", "use": "@now/static" },
      { "src": "*.manifest", "use": "@now/static" }
    ]
}
